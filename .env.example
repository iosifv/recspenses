# Postgres with docker local
POSTGRES_URL="postgres://postgres:postgres@localhost:5432/my_local_db"
POSTGRES_USER="postgres"
POSTGRES_HOST="localhost"
POSTGRES_PASSWORD="postgres"
POSTGRES_DATABASE="my_local_db"


# Clerk
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=

